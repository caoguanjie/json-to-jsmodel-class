{
    "name": "json-to-jsmodel-class",
    "displayName": "JSON to jsmodel class",
    "publisher": "caoguanjie",
    "author": "caoguanjie",
    "description": "converts selected json object to javascript model",
    "repository": "https://github.com/caoguanjie/json-to-jsmodel-class",
    "keywords": [
        "javascript",
        "json",
        "convert",
        "jsdoc",
        "create",
        "jsmodel",
        "class"
    ],
    "icon": "images/icon.png",
    "version": "1.0.1",
    "engines": {
        "vscode": "^1.55.2"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:json-to-jsmodel.convertToJsModel"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "json-to-jsmodel.convertToJsModel",
                "title": "Convert to JS Class Model"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "json-to-jsmodel.convertToJsModel",
                    "group": "json-to-jsmodel"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js",
        "publish": "vsce publish"
    },
    "devDependencies": {
        "@types/vscode": "1.39.0",
        "@types/glob": "7.1.3",
        "@types/mocha": "8.2.2",
        "@types/node": "12.11.7",
        "eslint": "7.26.0",
        "@typescript-eslint/eslint-plugin": "4.24.0",
        "@typescript-eslint/parser": "4.24.0",
        "glob": "7.1.6",
        "mocha": "8.4.0",
        "typescript": "4.2.4",
        "vscode-test": "1.5.2"
    },
    "dependencies": {
        "dirty-json": "0.9.2"
    }
}